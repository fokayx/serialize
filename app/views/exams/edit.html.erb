<%= form_for @exam do |f| %>
  <%= f.label :name, '名稱' %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :category, '分類' %>
  <%= @exam.category %>
  <% @category.each do |category| %>
    <%= f.check_box :category, {multiple: "true"}, "#{category}", nil %>
    <%= f.label :category, "#{category}" %>
  <% end %>
  <%= f.submit %>
<% end %>


